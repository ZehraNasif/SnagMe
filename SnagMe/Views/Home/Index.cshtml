
@{
    ViewBag.Title = "Home Page";
    var applicants = (List<string>)ViewData["Applicants"];
}

@Styles.Render("~/Content/datatables")

<div class="row">
    <div class="col-md-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                <h1 class="panel-title list-panel-title">Applicants who have matching 10 qualifications</h1>
            </div>
            <div class="panel-body">
                <table id="data-table"
                       class="table table-striped table-bordered"
                       style="width:100%">
                    <thead>
                        <tr>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (string app in applicants)
                        {
                            <tr>
                                <td>@app</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

@section Scripts
{
    <script type="text/javascript">
        $(document).ready(function () {

            $('#data-table').DataTable();
        });
    </script>
}

